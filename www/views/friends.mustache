<div id="add_friends">
    <h2>Add Friend:</h2>

    <form name="input" action="./friends" method="post">
        <input type="text" name="username" value="Username">
        <input type="submit" value="Add">
    </form> 

</div>

<div id="current_friends">
    <h2>Current Friends:</h2>
    <ul>
        {{#friendsList}}
        <li>{{.}}</li>
        {{/friendsList}}
    </ul>
</div>
